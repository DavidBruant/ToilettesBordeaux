{
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "eqeqeq": 2,
        "curly": 1,
        "dot-notation": 0,
        "no-shadow": 1,
        
        "quotes": 0,
        "no-trailing-spaces": 0,
        "no-multi-spaces": 0,
        "eol-last": 1,
        "space-infix-ops": 0
    },
    "globals": {
        "Promise": true,
        "google": true
    }
}
